let lado = console.read();
let area = lado*lado;

if(lado<=0){
    console.log("insira um valor valido");
} else if(area>= 20) {
   console.log("voce tem um quadradaoo!!!!! de espetaculares " + area + "de area");
} else {
    console.log("voce tem um quadradinhooo!! de apenas " + area + "de area");
}
